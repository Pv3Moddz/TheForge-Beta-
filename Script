local Player = game.Players.LocalPlayer
local Tween = game:GetService("TweenService")
local VirtualUser = game:GetService("VirtualUser")
local VirtualInputManager = game:GetService("VirtualInputManager")
local GuiService = game:GetService("GuiService")
local RunService = game:GetService("RunService")
local ForgeGUI = Player.PlayerGui:WaitForChild("Forge")
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
    Name = "The Forge Beta (༗ Pv3Moddz </>)",
    Icon = 0,
    LoadingTitle = "The Forge Beta",
    LoadingSubtitle = "by ༗ Pv3Moddz </>",
    ShowText = "༗ Pv3Moddz </>",
    Theme = "Dark Blue",
    ToggleUIKeybind = "K",
    ConfigurationSaving = {
        Enabled = true,
        FileName = "Saves"
    },
})

local Teleporting = false

local AntiAfk = false
local NoClip = false

local MineFarmRunning = false
local MineFarmToggle = nil
local MineFarmAnchorToggle = nil
local MineFarmDropdown = nil
local AreaMineFarmDropdown = nil
local MineFarmHeightSlider = nil

local MobFarmRunning = false
local MobFarmToggle = nil
local MobFarmAnchorToggle = nil
local SafeOnStunedToggle = nil
local MobFarmDropdown = nil
local MobFarmForwardSlider = nil
local MobFarmRightSlider = nil
local MobFarmUpSlider = nil

local AutoHammerMinigame = false

local TeleportSpeedSlider = nil
local InstaTeleportSlider = nil

local AutoSellRunning = false
local AutoSellToggle = nil
local AutoSellMinerDropdown = nil
local AutoSellEssenceDropdown = nil
local AutoSellSlider = nil

Player.Idled:connect(function()
    if AntiAfk then
        VirtualUser:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        wait(5)
        VirtualUser:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end
end)
RunService.Stepped:Connect(function()
	if NoClip and Player.Character then
		for _, part in ipairs(Player.Character:GetDescendants()) do
			if part:IsA("BasePart") then
				part.CanCollide = false
			end
		end
	end
end)
ForgeGUI:WaitForChild("HammerMinigame").ChildAdded:Connect(function(Button)
    if Button:IsA("TextButton") and AutoHammerMinigame then
        local Frame = Button:WaitForChild("Frame")
        local Border = Frame:WaitForChild("Border")
        local Circle = Frame:WaitForChild("Circle")
        local Connection = RunService.RenderStepped:Connect(function()
            if Button and Button.Parent and Frame and Border and Circle then
                if math.abs(Border.AbsoluteSize.X - Circle.AbsoluteSize.X) <= 20 and math.abs(Border.AbsoluteSize.Y - Circle.AbsoluteSize.Y) <= 20 then
                    local PosX = Button.AbsolutePosition.X + Button.AbsoluteSize.X / 2
                    local PosY = Button.AbsolutePosition.Y + Button.AbsoluteSize.Y / 2 + GuiService:GetGuiInset().Y
                    VirtualInputManager:SendMouseButtonEvent(PosX, PosY, 0, true, game, 0)
                    task.wait(0.05)
                    VirtualInputManager:SendMouseButtonEvent(PosX, PosY, 0, false, game, 0)
                    Connection:Disconnect()
                end
            else
                Connection:Disconnect()
            end
        end)
    end
end)

local MainTab = Window:CreateTab("Main", 4483362458)
MainTab:CreateSection("Moviment")
MainTab:CreateToggle({
    Name = "Safe On Stuned",
    CurrentValue = false,
    Flag = "SafeOnStunedToggle",
    Callback = function(Value)
        SafeOnStun = Value
        task.spawn(function()
            while SafeOnStun do
                local Humanoid = Player.Character:FindFirstChild("Humanoid")
                if Humanoid then
                    if Player.Character:FindFirstChild("Status"):FindFirstChild("Stun") then
                        Humanoid.HipHeight = 10
                    else
                        Humanoid.HipHeight = 0
                    end
                end
                task.wait()
            end
        end)
    end,
})
