local EspTab = Window:CreateTab("Esp", 4483362458)
EspTab:CreateSection("Player Esp")
EspTab:CreateToggle({
    Name = "Enable Player Esp",
    CurrentValue = false,
    Flag = "PlayerEsp",
    Callback = function(Value)
        PlayerEsp = Value
        task.spawn(function()
            if PlayerEsp then
                while PlayerEsp do
                    for _, CurrentPlayer in ipairs(game.Players:GetChildren()) do
                        local CurrentChar = CurrentPlayer.Character
                        if CurrentChar and CurrentChar:FindFirstChild("Head") then
                            if CurrentChar:FindFirstChild("PlayerEspBillboard") then
                                local Distance = math.floor((Player.Character:FindFirstChild("HumanoidRootPart").Position - CurrentChar:FindFirstChild("HumanoidRootPart").Position).Magnitude) or 0
                                CurrentChar.PlayerEspBillboard.PlayerEspText.Text = CurrentPlayer.Name.." | "..Distance.." Studs"
                            else
                                local PlayerEspBillboard = Instance.new("BillboardGui")
                                PlayerEspBillboard.Name = "PlayerEspBillboard"
                                PlayerEspBillboard.Size = UDim2.new(1,0,1,0)
                                PlayerEspBillboard.Adornee = CurrentChar.Head
                                PlayerEspBillboard.StudsOffset = Vector3.new(0,1.5,0)
                                PlayerEspBillboard.AlwaysOnTop = true
                                local PlayerEspText = Instance.new("TextLabel")
                                PlayerEspText.Name = "PlayerEspText"
                                PlayerEspText.Parent = PlayerEspBillboard
                                PlayerEspText.Size = UDim2.new(1,0,1,0)
                                PlayerEspText.BackgroundTransparency = 1
                                PlayerEspText.TextColor3 = Color3.new(1, 1, 1)
                                PlayerEspText.TextStrokeTransparency = 0
                                PlayerEspText.Text = CurrentPlayer.Name
                                PlayerEspBillboard.Parent = CurrentChar
                            end
                        end
                    end
                    task.wait(1.0)
                end
                for _, CurrentPlayer in ipairs(game.Players:GetChildren()) do
                    local CurrentChar = CurrentPlayer.Character
                    if CurrentChar and CurrentChar:FindFirstChild("PlayerEspBillboard") then
                        CurrentChar.PlayerEspBillboard:Destroy()
                    end
                end
            end
        end)
    end,
})
EspTab:CreateToggle({
    Name = "Enable Player Highlight",
    CurrentValue = false,
    Flag = "PlayerHighlight",
    Callback = function(Value)
        PlayerHighlight = Value
        task.spawn(function()
            if PlayerHighlight then
                while PlayerHighlight do
                    for _, CurrentPlayer in ipairs(game.Players:GetChildren()) do
                        local CurrentChar = CurrentPlayer.Character
                        if CurrentChar and not CurrentChar:FindFirstChild("PlayerHighlight") then
                            local PlayerHighlight = Instance.new("Highlight")
                            PlayerHighlight.Name = "PlayerHighlight"
                            PlayerHighlight.OutlineColor = Color3.fromRGB(255, 255, 255)
                            PlayerHighlight.OutlineTransparency = 0.5
                            PlayerHighlight.FillTransparency = 1
                            PlayerHighlight.Adornee = CurrentChar
                            PlayerHighlight.Parent = CurrentChar
                        end
                    end
                    task.wait(1.0)
                end
                for _, CurrentPlayer in ipairs(game.Players:GetChildren()) do
                    local CurrentChar = CurrentPlayer.Character
                    if CurrentChar and CurrentChar:FindFirstChild("PlayerHighlight") then
                        CurrentChar.PlayerHighlight:Destroy()
                    end
                end
            end
        end)
    end,
})

local TeleportTab = Window:CreateTab("Teleport", 4483362458)
TeleportTab:CreateSection("Teleport Settings")
local function TeleportFunction(Pos, NewPos)
    local HRP = Player.Character and Player.Character:FindFirstChild("HumanoidRootPart")
    if HRP and TeleportSpeedSlider and not Teleporting then
        Teleporting = true
        local TPTween = Tween:Create(HRP, TweenInfo.new((Pos - HRP.Position).Magnitude / TeleportSpeedSlider.CurrentValue, Enum.EasingStyle.Linear), {CFrame = NewPos})
        TPTween:Play()
        TPTween.Completed:Wait()
        Teleporting = false
    end
end
TeleportSpeedSlider = TeleportTab:CreateSlider({
    Name = "Teleport Speed",
    Range = {30, 150},
    Increment = 1,
    Suffix = "x",
    CurrentValue = 60,
    Flag = "TeleportSpeedSlider",
    Callback = function(Value)
    end,
})
InstaTeleportSlider = TeleportTab:CreateSlider({
    Name = "Insta Teleport Max Distance",
    Range = {0, 200},
    Increment = 1,
    Suffix = "Studs",
    CurrentValue = 50,
    Flag = "InstaTeleportSlider",
    Callback = function(Value)
    end,
})
TeleportTab:CreateSection("Teleport Shops")
local TeleportLocations = {
    ["Forging Station"] = Vector3.new(-193, 30, 160),
    ["Enhancer Shop"] = Vector3.new(-260, 20, 30),
    ["Runemaker Shop"] = Vector3.new(-271, 20, 145),
    ["Pickaxe Shop"] = Vector3.new(-95, 30, 89),
    ["Weapon Buyer"] = Vector3.new(-177, 30, 18),
    ["Gem Buyer"] = Vector3.new(-113, 40, -35),
    ["Potion Shop"] = Vector3.new(-155, 30, 115),
    ["Weapon Seller Shop"] = Vector3.new(-155, 30, 115),
    ["Wizard Tower"] = Vector3.new(-155, 30, 115),
}
for Name, Pos in pairs(TeleportLocations) do
    TeleportTab:CreateButton({
        Name = Name,
        Callback = function()
            local NewPos = CFrame.new(Pos)
            TeleportFunction(Pos, NewPos)
        end,
    })
end

local AutoSellTab = Window:CreateTab("Auto Sell", 4483362458)
local function SellItem()
    if AutoSellMinerDropdown and AutoSellEssenceDropdown then
        local Itens = AutoSellMinerDropdown.CurrentOption
        for _, Item in ipairs(AutoSellEssenceDropdown.CurrentOption) do table.insert(Itens, Item) end
        local Inventory = game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("EnhanceService"):WaitForChild("RF"):WaitForChild("FindEquipmentByGUID"):InvokeServer().replica.Data.Inventory
        local ItenList = {}
        local Count = 0
        for Key, Value in pairs(Inventory) do
            if typeof(Value) == "table" then
                for _, Item in pairs(Value) do
                    if Item.Name and Item.Quantity and table.find(Itens, tostring(Item.Name)) then
                        ItenList[Item.Name] = Item.Quantity
                        Count += 1
                    end
                end
            else
                if table.find(Itens, tostring(Key)) then
                    ItenList[Key] = Value
                    Count += 1
                end
            end
        end
        if Count > 0 and TeleportSpeedSlider and InstaTeleportSlider then
            while Teleporting do task.wait() end
            Teleporting = true
            local HRP = Player.Character and Player.Character:FindFirstChild("HumanoidRootPart")
            local NpcHRP = workspace.Proximity:FindFirstChild("Greedy Cey"):FindFirstChild("HumanoidRootPart")
            local NewCFrame = NpcHRP.CFrame
            if (NpcHRP.Position - HRP.Position).Magnitude < InstaTeleportSlider.CurrentValue then
                HRP.CFrame = NewCFrame
            end
            local TPTween = Tween:Create(HRP, TweenInfo.new((NpcHRP.Position - HRP.Position).Magnitude / TeleportSpeedSlider.CurrentValue, Enum.EasingStyle.Linear), {CFrame = NewCFrame})
            TPTween:Play()
            TPTween.Completed:Wait()
            game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("ProximityService"):WaitForChild("RF"):WaitForChild("ForceDialogue"):InvokeServer(workspace:WaitForChild("Proximity"):WaitForChild("Greedy Cey"), "SellConfirmMisc")
            game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("DialogueService"):WaitForChild("RF"):WaitForChild("RunCommand"):InvokeServer("SellConfirm", {Basket = ItenList})
            game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("StatusService"):WaitForChild("RF"):WaitForChild("GetPlayerEquipmentInfo"):InvokeServer()
            game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("DialogueService"):WaitForChild("RE"):WaitForChild("DialogueEvent"):FireServer("Closed")
            Teleporting = false
        end
    end
end
AutoSellTab:CreateSection("Auto Sell")
AutoSellToggle = AutoSellTab:CreateToggle({
    Name = "Auto Sell",
    CurrentValue = false,
    Flag = "AutoSellMinerToggle",
    Callback = function(Value)
        AutoSellRunning = Value
        if AutoSellSlider then
            task.spawn(function()
                while AutoSellRunning do
                    task.wait(AutoSellSlider.CurrentValue)
                    SellItem()
                end
            end)
        end
    end,
})
AutoSellTab:CreateButton({
    Name = "Manual Sell",
    Callback = function()
        SellItem()
    end,
})
